import java.util.Timer;
import java.util.TimerTask;


public class InterruptTimer {

	/**
	 * @param args
	 */
	public static void main(String[] args) {

		Timer timer = new Timer();
		timer.schedule(new TimerTask() {
			  @Override
			  public void run() {
			  }
			}, 5000);
	}

}
